  "title": "Wishlist Button",
  "category": "sp_woo_loop",
  "description": "",
  "supports": {
    "html": false
  },
  "textdomain": "shop-press",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "icon": {
      "type": "object"
    },
    "icon_pos": {
      "type": "string",
