{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "shop-press/product-suggest-price",
  "version": "1.0.0",
  "title": "Product Suggest Price",
  "category": "sp-product-single",
  "description": "",
  "supports": {
    "html": false
  },
  "textdomain": "shop-press",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "attributes": {
    "btn_text": {
      "type": "string",
      "default": "Suggest Price"
    },
    "email_address": {
      "type": "string",
      "default": "you@yourdomain.com"
    },
    "name_placeholder": {
      "type": "string",
      "default": "Name"
    },
    "email_placeholder": {
      "type": "string",
      "default": "Email"
    },
    "price_placeholder": {
      "type": "string",
      "default": "Price"
    },
    "message_placeholder": {
      "type": "string",
      "default": "Message"
    },
    "submit_btn_text": {
      "type": "string",
      "default": "Send"
    },
    "wrapperID": {
      "type": "string",
      "default": ""
    },
    "styler": {
      "type": "string",
      "default": "{}"
    }
  }
}