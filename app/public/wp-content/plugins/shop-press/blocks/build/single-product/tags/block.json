  "name": "shop-press/product-tags",
  "version": "1.0.0",
  "title": "Product Tags",
  "category": "sp-product-single",
  "description": "",
  "supports": {
    "html": false
  },
  "textdomain": "shop-press",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
